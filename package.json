{
  "name": "mydiary",
  "version": "1.0.0",
  "description": "This is an online journal where users can pen down their thoughts and feelings.",
  "main": "index.js",
  "eslintIgnore": [
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "start": "babel-node ./bin/www",
    "test": "set NODE_ENV=test&&nyc mocha --timeout 10000 --compilers js:babel-core/register --require babel-polyfill ./test/**/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start-dev": "nodemon --exec babel-node --presets es2015,stage-2 ./bin/www",
    "coveralls": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "cover": "./node_modules/coveralls/bin/coveralls",
    "lint": "eslint ./node_modules/.bin/eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jideajayi11/MyDiary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jideajayi11/MyDiary/issues"
  },
  "homepage": "https://github.com/jideajayi11/MyDiary#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "node-localstorage": "^1.3.1",
    "nodemon": "^1.18.2",
    "pg": "^7.4.3",
    "pg-promise": "^8.4.5"
  },
  "devDependencies": {
    "ajv": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "istanbul": "^0.4.5",
    "lint": "^1.1.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7"
  }
}
